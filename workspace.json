{
  "version": 2,
  "projects": {
    "dragons-and-coin-game": {
      "projectType": "library",
      "root": "libs/dragons-and-coin-game",
      "sourceRoot": "libs/dragons-and-coin-game/src",
      "prefix": "yonatan",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/dragons-and-coin-game"],
          "options": {
            "jestConfig": "libs/dragons-and-coin-game/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/dragons-and-coin-game/src/**/*.ts",
              "libs/dragons-and-coin-game/src/**/*.html"
            ]
          }
        }
      }
    },
    "nativescript-audio": {
      "projectType": "library",
      "root": "libs/nativescript-audio",
      "sourceRoot": "libs/nativescript-audio/src",
      "prefix": "yonatan",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nativescript-audio"],
          "options": {
            "jestConfig": "libs/nativescript-audio/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/nativescript-audio/src/**/*.ts",
              "libs/nativescript-audio/src/**/*.html"
            ]
          }
        }
      }
    },
    "nativescript-dragons-and-coin": {
      "projectType": "application",
      "root": "apps/nativescript-dragons-and-coin/",
      "sourceRoot": "apps/nativescript-dragons-and-coin/src",
      "prefix": "",
      "targets": {
        "build": {
          "executor": "@nativescript/nx:build",
          "options": {
            "noHmr": true,
            "production": true,
            "uglify": true,
            "release": true,
            "forDevice": true,
            "assets": [
              {
                "glob": "**/*",
                "input": "libs/dragons-and-coin-game/src/assets/",
                "output": "/assets/"
              }
            ]
          },
          "configurations": {
            "prod": {
              "fileReplacements": [
                {
                  "replace": "./src/environments/environment.ts",
                  "with": "./src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "ios": {
          "executor": "@nativescript/nx:build",
          "options": {
            "platform": "ios"
          },
          "configurations": {
            "build": {
              "copyTo": "./dist/build.ipa"
            },
            "prod": {
              "combineWithConfig": "build:prod"
            }
          }
        },
        "android": {
          "executor": "@nativescript/nx:build",
          "options": {
            "platform": "android"
          },
          "configurations": {
            "build": {
              "copyTo": "./dist/build.apk"
            },
            "prod": {
              "combineWithConfig": "build:prod"
            }
          }
        },
        "clean": {
          "executor": "@nativescript/nx:build",
          "options": {
            "clean": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nativescript-dragons-and-coin/**/*.ts",
              "apps/nativescript-dragons-and-coin/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nativescript-dragons-and-coin/jest.config.js",
            "tsConfig": "apps/nativescript-dragons-and-coin/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/nativescript-dragons-and-coin/src/test-setup.ts"
          }
        }
      }
    },
    "nx-conf-demo": {
      "projectType": "application",
      "root": "apps/nx-conf-demo",
      "sourceRoot": "apps/nx-conf-demo/src",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nx-conf-demo",
            "index": "apps/nx-conf-demo/src/index.html",
            "main": "apps/nx-conf-demo/src/main.ts",
            "polyfills": "apps/nx-conf-demo/src/polyfills.ts",
            "tsConfig": "apps/nx-conf-demo/tsconfig.app.json",
            "assets": [
              "apps/nx-conf-demo/src/favicon.ico",
              "apps/nx-conf-demo/src/assets"
            ],
            "styles": ["apps/nx-conf-demo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/nx-conf-demo/src/environments/environment.ts",
                  "with": "apps/nx-conf-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "nx-conf-demo:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "nx-conf-demo:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/nx-conf-demo/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nx-conf-demo"],
          "options": {
            "jestConfig": "apps/nx-conf-demo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-conf-demo-e2e": {
      "root": "apps/nx-conf-demo-e2e",
      "sourceRoot": "apps/nx-conf-demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nx-conf-demo-e2e/cypress.json",
            "devServerTarget": "nx-conf-demo:serve",
            "tsConfig": "apps/nx-conf-demo-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nx-conf-demo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/nx-conf-demo-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "nx-conf-demo",
  "generators": {
    "@nrwl/web:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/web:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:application": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {}
  }
}
